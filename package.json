{
  "name": "blive-ws",
  "version": "1.1.0",
  "description": "B站直播间 WebSocket 服务，可用于实时获取直播间弹幕",
  "keywords": [
    "danmaku",
    "B站弹幕",
    "直播间弹幕",
    "弹幕抓取",
    "弹幕游戏",
    "B站WebSocket"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "DEBUG=1 yarn start",
    "start": "deno run -A src/deno/app.ts",
    "parse": "node anatomy/parser.js",
    "parse:msg": "node anatomy/message-parser.js",
    "test:write": "deno run -A test/persistence/write.ts",
    "test:read": "deno run -A test/persistence/read.ts",
    "test:msg": "deno run -A test/message/danmu.ts"
  },
  "devDependencies": {
    "acorn": "^8.10.0",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "fs-extra": "^11.1.1",
    "http-proxy-middleware": "^2.0.6",
    "morgan": "^1.10.0",
    "open": "^8.4.0"
  }
}
